{
  "name": "search-replace-extension",
  "version": "0.1.0",
  "description": "Search and replace features for documents in jupyterlab. The UI works with implementations of the ISearchReplacePlugin interface that have been added to the SearchReplaceRegistry. Notebook and editor widget plugins are provided by the extension by default.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jandrewtorres/jupyterlab-find-replace-extension",
  "bugs": {
    "url": "https://github.com/jandrewtorres/jupyterlab-find-replace-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "John A Torres",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jandrewtorres/jupyterlab-find-replace-extension.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "tslint": "tslint --force --fix -c tslint.json '**/*{.ts,.tsx}'",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.17.2",
    "@jupyterlab/apputils": "^0.17.2",
    "@jupyterlab/cells": "^0.17.2",
    "@jupyterlab/codeeditor": "^0.17.2",
    "@jupyterlab/docmanager": "^0.17.2",
    "@jupyterlab/docregistry": "^0.17.2",
    "@jupyterlab/notebook": "^0.17.2",
    "@jupyterlab/observables": "^2.0.7",
    "@jupyterlab/services": "^3.0.3",
    "@phosphor/algorithm": "^1.1.2",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/signaling": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "react": "^16.4.1"
  },
  "devDependencies": {
    "@types/react-dom": "^16.0.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
