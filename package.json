{
  "name": "search-replace-notebook-extension",
  "version": "0.1.0",
  "description": "Search and replace / replace all for notebooks in jupyterlab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jandrewtorres/search-and-replace-extension",
  "bugs": {
    "url": "https://github.com/jandrewtorres/search-and-replace-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "John A Torres",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jandrewtorres/search-and-replace-extension.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "jupyterlab": "^0.17.5"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
